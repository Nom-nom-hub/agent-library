{
  "extends": "markdownlint/style/relaxed",
  "no-hard-tabs": false,
  "line-length": {
    "line_length": 120,
    "heading_line_length": 120,
    "code_line_length": 120
  },
  "ul-indent": {
    "indent": 2
  },
  "no-multiple-blanks": {
    "maximum": 2
  },
  "no-trailing-punctuation": {
    "punctuation": ".,;:"
  }
}
